  "version": "1.0.0",
  "author": "Mike Miller",
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "gatsby": "^2.27.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet-async": "^1.1.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-source-filesystem": "^2.6.0",
    "gatsby-transformer-sharp": "^2.7.0",
    "gatsby-transformer-yaml": "^2.6.0",
    "graphql": "^14.7.0",
    "postcss": "^8.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.11.0",
    "react-helmet-async": "^1.0.4",
    "react-image-gallery": "^0.8.16",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different src/**/*.{jsx,js}",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write src/**/*.{jsx,js}",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
